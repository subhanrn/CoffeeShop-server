{
  "name": "coffeeshop-server",
  "version": "1.0.0",
  "main": "server/server.js",
  "engines": {
    "node": "10.x"
  },
  "scripts": {
    "format": "prettier {common,server}/**/*.{js,jsx,json,ts,tsx,css,scss,md,html} --write",
    "precommit": "lint-staged",
    "lint": "eslint .",
    "start": "node .",
    "posttest": "npm run lint",
    "build:sdk": "./node_modules/.bin/lb-sdk server/server.js ./client/sdk -l angular2 -d ng2web -i disabled",
    "build:swagger": "openapi-generator generate -i swag.yml -g dart -DbrowserClient=false -o ./client"
  },
  "lint-staged": {
    "{src,e2e}/**/*.{js,jsx,ts,tsx}": [
      "npm run lint",
      "git add"
    ],
    "{src,e2e}/**/*.{js,jsx,ts,tsx,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "async": "^2.6.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "helmet": "^3.16.0",
    "loopback": "^3.25.1",
    "loopback-boot": "^2.28.0",
    "loopback-component-explorer": "^6.3.1",
    "loopback-connector-mysql": "^5.3.1",
    "loopback-connector-oracle": "^4.1.2",
    "loopback-connector-postgresql": "^3.6.1",
    "serve-favicon": "^2.5.0",
    "strong-error-handler": "^3.2.0"
  },
  "devDependencies": {
    "@mean-expert/loopback-sdk-builder": "^2.3.1",
    "@openapitools/openapi-generator-cli": "0.0.9-4.0.0-beta3",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-loopback": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "prettier": "^1.17.0"
  },
  "repository": {
    "type": "",
    "url": ""
  },
  "license": "UNLICENSED",
  "description": "coffeeshop-server"
}
